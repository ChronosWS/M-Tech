{
  // Generates all block states for all blocks in the registry.
  "generate_all_known_states": true,
  // Whether blocks are replaced in existing chunks.
  "retro_gen": false,
  "state_swapper": [
    {
      "new": {
        "Name": "minecraft:barrel",
      },
      "old": {
        "Name": "immersiveengineering:crate",
      }
    },
    {
      "new": {
        "Name": "minecraft:barrel",
      },
      "old": {
        "Name": 'immersiveengineering:reinforced_crate',
      }
    }
  ],
  "swapper": {
    "immersiveengineering:crate": "minecraft:barrel",
    'immersiveengineering:reinforced_crate': "minecraft:barrel",
  }
}